{
  "name": "exercise",
  "version": "1.0.0",
  "description": "MeLi exercise",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production gulp compile; NODE_ENV=production node index",
    "start:d": "NODE_ENV=development gulp",
    "test": "NODE_ENV=test jest --coverage"
  },
  "keywords": [
    "react",
    "express"
  ],
  "author": {
    "name": "Nicolas",
    "lastname": "Molina"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/comodinx/Exercise.git"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "del": "3.0.0",
    "enzyme": "3.1.1",
    "enzyme-adapter-react-16": "1.0.4",
    "gulp": "3.9.1",
    "gulp-babel": "7.0.0",
    "gulp-nodemon": "2.2.1",
    "gulp-sass": "3.1.0",
    "jest": "21.2.1",
    "jest-enzyme": "4.0.1",
    "webpack": "3.8.1",
    "webpack-stream": "4.0.0"
  },
  "dependencies": {
    "bluebird": "3.3.5",
    "body-parser": "1.15.0",
    "express": "4.16.2",
    "intl": "1.2.5",
    "moment": "2.13.0",
    "morgan": "1.9.0",
    "orcorum": "0.1.10",
    "query-string": "5.0.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-helmet": "^5.2.0",
    "react-intl": "2.4.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "request": "2.83.0",
    "request-promise": "4.2.2",
    "serve-favicon": "2.3.0",
    "underscore": "1.8.3",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "setupFiles": [
      "./test/setupTests.js"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageDirectory": "./test/coverage",
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      },
      "./src/components/**/*.js": {
        "branches": 50,
        "statements": 50
      }
    }
  }
}
